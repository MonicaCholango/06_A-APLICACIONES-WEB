@model ExamenParcial.Models.Cliente

@{
    ViewData["Title"] = "Detalles del Cliente";
}

<h1>Detalles del Cliente</h1>

<div>
    <h4>Cliente</h4>
    <hr />

    <dl class="dl-horizontal">
        <dt>Nombre</dt>
        <dd>@Model.Nombre</dd>

        <dt>Apellido</dt>
        <dd>@Model.Apellido</dd>

        <dt>Email</dt>
        <dd>@Model.Email</dd>

        <dt>Teléfono</dt>
        <dd>@Model.Telefono</dd>
    </dl>

    <a href="@Url.Action("Index")" class="btn btn-secondary">Volver a la lista de Clientes</a>
</div>
